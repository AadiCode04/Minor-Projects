    <title>All Posts</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1>Quora Posts</h1>
    <% for(post of posts){ %>
        <div  class="post">
            <h3 class="content">@<%= post.username %></h3>
            <h3 class="content"><%= post.content %></h3>
            <form action="post" action="">
               

                <button>Delete Post</button>
            </form>

        </div>


    <%} %>
